    "express": "^4.16.2",
    "graphql": "^0.11.7",
    "graphql-tools": "^2.20.2",
    "helmet": "^3.11.0",
    "jsonwebtoken": "^8.1.1",
    "mongoose": "^5.0.4",
    "nedb": "^1.8.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "rimraf": "^2.6.1"
